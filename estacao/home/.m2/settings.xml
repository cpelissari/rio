<settings>
	<pluginGroups>
		<pluginGroup>org.mortbay.jetty</pluginGroup>
	</pluginGroups>
	<mirrors>
		<mirror>
			<id>nexus</id>
			<mirrorOf>*</mirrorOf>
			<url>http://rio.objectos.com.br/nexus/content/groups/public</url>
		</mirror>
	</mirrors>
	<profiles>
		<profile>
			<id>mysql-eclipse</id>
			<properties>
				<jdbc.baseUrl>jdbc:mysql://127.0.0.1:9000</jdbc.baseUrl>
			</properties>
		</profile>
		<profile>
			<id>mysql-jetty</id>
			<properties>
				<jdbc.baseUrl>jdbc:mysql://127.0.0.1:9001</jdbc.baseUrl>
			</properties>
		</profile>
		<profile>
			<id>mysql-backup</id>
			<properties>
				<jdbc.baseUrl>jdbc:mysql://127.0.0.1:9002</jdbc.baseUrl>
			</properties>
		</profile>
		<profile>
			<id>nexus</id>
			<repositories>
				<repository>
					<id>central</id>
					<url>http://central</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</repository>
			</repositories>
			<pluginRepositories>
				<pluginRepository>
					<id>central</id>
					<url>http://central</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</pluginRepository>
			</pluginRepositories>
		</profile>
	</profiles>
	<activeProfiles>
		<activeProfile>nexus</activeProfile>
		<activeProfile>mysql-eclipse</activeProfile>
	</activeProfiles>
</settings>
